<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:Project.Client.ViewModel"
                    xmlns:vw="clr-namespace:Project.Client.View"
                    xmlns:mui="http://firstfloorsoftware.com/ModernUI"                    
                    xmlns:local="clr-namespace:Project">
    
    <DataTemplate x:Shared="False"  DataType="{x:Type vm:IKnowWhatIWantViewModel}">
        <vw:IKnowWhatIWantView/>
    </DataTemplate>
    <DataTemplate x:Shared="False" DataType="{x:Type vm:GetParamViewModel}">
        <vw:GetParamView/>
    </DataTemplate>
    <DataTemplate x:Shared="False" DataType="{x:Type vm:SignInViewModel}">
        <vw:SignInView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:SignUpViewModel}">
        <vw:SignUpView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:ResultViewModel}">
        <vw:ResultView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:HitMeWithViewModel}">
        <vw:HitMeWithView/>
    </DataTemplate>

</ResourceDictionary>